{
    "name":        "arcanedev/embed-video",
    "description": "EmbedVideo is a helper that allows you to generate <iframe> html based on url (Youtube, Vimeo ...)",
    "keywords":    ["arcanedev", "laravel", "embed", "video", "youtube", "vimeo"],
    "homepage":    "https://github.com/ARCANEDEV/EmbedVideo",
    "authors": [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               ">=5.6.4",
        "arcanedev/support": "~4.0"
    },
    "require-dev": {
        "phpunit/phpcov":  "~3.0",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\EmbedVideo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\EmbedVideo\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "testbench": "composer require --dev \"orchestra/testbench=~3.0\""
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
